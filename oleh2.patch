+{
+    AbstractProtocol::FieldFlags flags;
+
+    flags = AbstractProtocol::fieldFlags(index);
+
+    switch (index)
+    {
+        case is_c:
+        case is_r:
+        case is_p :
+        case is_a :
+        flags &= ~FrameField;
+        flags |= MetaField;
+            break;
+        default:
+            break;
